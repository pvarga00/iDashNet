@model iDashNet.Models.ChartModel
@{
    ViewData["Title"] = "My QL Historical Page Load Time";
}

@using Newtonsoft.Json.Linq
@using Newtonsoft.Json;


<h2>Chart</h2>


<canvas id="dashboardChart">

</canvas>



<script>
    var chartContext = document.getElementById("dashboardChart").getContext('2d');

    var dashboardChart = new Chart(chartContext, {

        type: 'line',

        // The data for our dataset
        data: {
            labels: ["150 days", "120 days", "90 days", "60 days", "30 days", "Yesterday"],
            datasets: [{
                label: "MyQL Historical Page Load Time(ms)",
                backgroundColor: 'rgb(255, 99, 132)',
                borderColor: 'rgb(255, 99, 132)',
                data: @Json.Serialize(Model.chartData),
            }]
        },

        // Configuration options go here
        options: {}

    });
</script>

